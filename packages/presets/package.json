{
  "name": "@crush-ux/presets",
  "version": "1.0.0",
  "description": "Design presets and themes for crush-ux components",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf dist && vite build --config vite.lib.config.ts",
    "dev": "vite build --config vite.lib.config.ts --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ui",
    "presets",
    "themes",
    "design-system",
    "css",
    "styling"
  ],
  "author": "Christopher Rathgeb",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "peerDependencies": {
    "@crush-ux/core": "^1.0.0"
  },
  "devDependencies": {
    "@crush-ux/core": "*",
    "@types/react": "^19.1.9",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite": "^7.1.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
