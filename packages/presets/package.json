{
  "name": "@crush-ui/presets",
  "version": "1.0.0",
  "description": "Design presets and themes for crush-ui components",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --module commonjs --outDir dist",
    "build:esm": "tsc --module esnext --outDir dist/esm && mv dist/esm/index.js dist/index.esm.js",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ui",
    "presets",
    "themes",
    "design-system",
    "css",
    "styling"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "peerDependencies": {
    "@crush-ui/core": "^1.0.0"
  },
  "devDependencies": {
    "@crush-ui/core": "workspace:*",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}